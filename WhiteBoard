   }

prompt 3
Compressing strings
 input "aaabCCdddda"
 output "3ab2c4da"
 function compression(string) {
   let outputString = "";
   console.log(" original string:" + outputString);
   let counter = 1;
   for (let i=0; i< string.length; i++){
     if (string[i] != string[i+1] && counter <= 1){
       outputString =outputString.concat(string[i]);
      console.log("looped string:" + outputString);
     } else if(string[i] != string[i+1] && counter> 1){
        outputString = outputString.concat.toString(counter);
        outputString = outputString.concat(string[i]);
        counter =1;
      } else {
        counter += 1;
        console.log("other looped string:" + outputString + "  counter: " +counter);
     }
   }
  console.log("final string:" + outputString);
  return outputString;
 }
compression("aaabCCdddda");

