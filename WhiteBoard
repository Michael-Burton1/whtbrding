prompt 1 w/o recurrsion
turning strings into URL
input "oh Yeah"
output "oh%20yeah"

 function toURL(inputString) {
  let outString = "";
  for (let i=0; i<inputString.length; i++){
    if (string[i] != " ") {
      outString =outString.concat(string[i]);
    } else {
      outString = outString.concat("%20");
    }
    console.log("my out String: " + outString);
    return outString;
  }
  toURL("hey there");

